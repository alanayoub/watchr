<!doctype html>
<html lang="en">
<head>
    <meta charset="utf-8">
    <meta name="description" content="Watchr">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <title>Watcher</title>
    <link href="/.gen/styles/main.css" type="text/css" rel="stylesheet" media="screen, print">
</head>
<body>
{{{body}}}
<script data-main="/scripts/main" src="/lib/requirejs/require.js"></script>
<script type="text/javascript">
    require.config({
        baseUrl: '/scripts',
        waitSeconds: 6,
        paths: {
            jquery: '/lib/jquery/jquery',
            'jquery.flot': '/lib/flot/jquery.flot',
            'jquery.flot.time': '/lib/flot/jquery.flot.time',
            backbone: '/lib/backbone/backbone',
            underscore: '/lib/underscore/underscore',
            handlebars: '/lib/handlebars/handlebars.runtime',
            moment: '/lib/momentjs/moment',
            helpers: '/services/helpers',
            errors: '/services/errors',
            socket: '/services/socket',
            rest: '/services/rest',
            text: '/lib/requirejs-text/text',
            templates: '/.gen/scripts/templates',
            sockets: '/.gen/scripts/socket.io'
        },
        shim: {
            jquery: {
                exports: '$'
            },
            'jquery.flot': {
                deps: ['jquery'], exports: '$.plot'
            },
            'jquery.flot.time': {
                deps: ['jquery.flot']
            },
            templates: {
                deps: ['handlebars'], exports: 'templates'
            },
            backbone: {
                deps: ['text', 'underscore', 'jquery', 'handlebars', 'helpers', 'templates', 'sockets'],
                exports: 'Backbone'
            }
        }
    });
</script>
</body>
</html>
